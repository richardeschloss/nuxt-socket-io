{
  "name": "nuxt-socket-io",
  "version": "2.0.3",
  "description": "Socket.io client and server module for Nuxt. Just plug it in and GO",
  "author": "Richard Schloss",
  "type": "module",
  "main": "lib/module.js",
  "types": "lib/types.d.ts",
  "license": "MIT",
  "contributors": [
    {
      "name": "Richard Schloss"
    }
  ],
  "keywords": [
    "nuxt",
    "socket.io",
    "socket.io-client",
    "vue",
    "vuejs",
    "easy"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/richardeschloss/nuxt-socket-io",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate:local": "nuxt generate",
    "generate:gh-pages": "cross-env nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava --colors --watch --timeout=10m",
    "test:cov": "c8 ava --timeout=10m"
  },
  "files": [
    "lib",
    "utils"
  ],
  "imports": {
    "#root/*": "./*"
  },
  "dependencies": {
    "@nuxt/types": "^2.15.5",
    "glob": "^7.1.7",
    "socket.io": "^4.1.1",
    "socket.io-client": "^4.1.1",
    "tiny-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@nuxtjs/eslint-config": "^6.0.1",
    "ava": "^3.15.0",
    "bootstrap": "^4.6.1",
    "browser-env": "^3.3.0",
    "c8": "^7.10.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "jsdom": "^16.7.0",
    "jsdom-global": "^3.0.2",
    "les-utils": "^2.0.4",
    "nuxt": "^2.15.8"
  }
}
